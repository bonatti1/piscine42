NAME = rush-02

SRC = main.c\
	  
SRC2 = main.o\
	  

$(NAME):
	@gcc -o $(NAME) $(SRC)

clean:
	@/bin/rm -f $(SRC2)

fclean: clean
	@/bin/rm -f $(NAME)
